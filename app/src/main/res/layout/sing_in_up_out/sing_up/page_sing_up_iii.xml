<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".SingUP.pSingUp_III"
    android:id="@+id/layout_sing_up_iii">

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="@string/ResSingUp"
        android:gravity="center"
        android:id="@+id/txt_sing_up_iii_sing_up"
        android:textSize="45dp"
        android:textColor="#ffffff"
        android:textAlignment="center"
        android:layout_gravity="center"
        android:paddingTop="15dp"
        android:paddingBottom="15dp"
        android:background="@android:color/holo_blue_dark"
        android:enabled="true"
        android:layout_centerVertical="true"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true" />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="@string/ResSingUp"
        android:gravity="center"
        android:id="@+id/textView2"
        android:textSize="45dp"
        android:textColor="#ffffff"
        android:textAlignment="center"
        android:layout_gravity="center"
        android:paddingTop="15dp"
        android:paddingBottom="15dp"
        android:background="@android:color/holo_blue_dark"
        android:enabled="true"
        android:layout_below="@+id/txt_sing_up_iii_sing_up"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="49dp" />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="@string/ResSingUp"
        android:gravity="center"
        android:id="@+id/textView4"
        android:textSize="45dp"
        android:textColor="#ffffff"
        android:textAlignment="center"
        android:layout_gravity="center"
        android:paddingTop="15dp"
        android:paddingBottom="15dp"
        android:background="@android:color/holo_blue_dark"
        android:enabled="true"
        android:layout_above="@+id/txt_sing_up_iii_sing_up"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="68dp" />

    <ImageView
        android:layout_width="193dp"
        android:layout_height="193dp"
        android:id="@+id/imageView4"
        android:layout_gravity="center_horizontal|top"
        android:src="@mipmap/icon_app"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_above="@+id/textView4" />

</RelativeLayout>
